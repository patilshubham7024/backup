class Pattern2 {

	public static void main(String[] args) {
		int n=5;
		for(int i=n;i<=n;i++) {
			for(int j=0;j<(i*2);j++) {
				for(int k=n;k<n;k--) {
					System.out.print(" ");
				}
				for(int l=0;l<(i*2);l++) {
					System.out.print((i+j)%2);
				}
			}System.out.println(i+")\n");
		}
	}

}
