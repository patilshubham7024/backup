package Pattern;

public class Test {
	static boolean isArm(int num) {
		int temp=num, sum=0;
		 do{
			int r= temp%10;
			sum=sum+r*r*r;
			temp=temp/10;
		}while(temp%10!=0 || temp>10);
		if (num==sum) return true;
		else return false;
	}
	static int addEven(int num) {
		return sum;
		
	}
	static int addOdd(int num) {
		return sum;
	}
	public static void main(String[] args) {
		int n=407;
		System.out.println(isArm(n));
		if(isArm(n))addEven(n);
		else addOdd(n);
	}

}