package Pattern;

public class Patt_Practice {

	public static void main(String[] args) {
		int n=10,sp=n/2,st=1;
		for(int i=1;i<=n;i++) {
			for(int j=1;j<=sp;j++) System.out.println("  ");
			
			for(int j=1; j<=st; j++) System.out.println("* ");
			
			if(i<=n/2) {sp--; st++;}
			else {sp++; st--}
		}
	}

}
