class HCFLCM {
	static int HCF(int i, int j) {
		int x=0;
		for(x=i;;x--) {
			System.out.println(x);
			if(i%x==0&&j%x==0) {
				break;
			}
		}
		return x;
	}
	static int LCM(int l,int m) {
		int y=0;
		for(y=m;;y++) {
			if(m%y) break;
		}
		return y;}
	public static void main(String[] args) {

//		int x=HCF(24,36);
		System.out.println("Hcf "+HCF(24,36));
//		int y=LCM(40,50);
		System.out.println("LCM"+LCM(40,50));
	}

}
