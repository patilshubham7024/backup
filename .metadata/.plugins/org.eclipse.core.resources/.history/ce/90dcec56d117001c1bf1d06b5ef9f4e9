import java.util.Scanner;
class Happy{static Scanner s = new Scanner(System.in);
	
	public static void main(String []Shubham){
		System.out.println("Enter No");
		int n = s.nextInt();
		n=23;
		int temp=n,sum=0;
		while(temp>9) {
		do{
			int dig=temp%10;
			sum=sum+dig*dig;System.out.println(sum+" "+temp);
			temp=temp/10;
		}while(sum>10);
		}
		if(sum==1 || sum==7)System.out.println("Happy");
		else System.out.println("Not Happy");
	
	}
}