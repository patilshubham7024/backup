import java.util.Scanner;
class Pattern{
	
    public static void main(String[] args){
	Scanner s=new Scanner(System.in);
	System.out.println("");
	int n=s.nextInt();	 
	for(int i=1;i<=n;i++) {
		for(int j=1;j<=n;j++) {
			if(i==1||i==n||j==1||j==n||i==j||i+j==n+1)System.out.println("*");
			else System.out.println(" ");
			}
		System.out.println();
		}
	}
    }
