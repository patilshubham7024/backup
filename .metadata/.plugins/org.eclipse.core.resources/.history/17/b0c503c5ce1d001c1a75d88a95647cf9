package Pattern;

public class Patternn {

	public static void main(String[] args) {
		int n=10;
		for(int i=1;i<=n;i++) {
			if(i<n/2)for(int j=1;j<=i;j++)System.out.print("* ");
		
			if(i>=n/2)for(int j=i;j>0;j-- 2ya3rsen4grdmrhbt jmvug5y)System.out.print("* ");
			
			System.out.println();
		}
	}

}
