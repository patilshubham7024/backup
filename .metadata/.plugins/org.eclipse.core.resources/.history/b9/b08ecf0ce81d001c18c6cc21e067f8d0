package Pattern;

public class Patternn {

	public static void main(String[] args) {
		int n=10;
		for(int i=1;i<=n;i++) {
			int j;
			if(i<n/2)for(j=1;j<=i;j++)System.out.print("* ");
		
			if(i>=n/2)for(j=i;i+j==n;j--)System.out.print("* ");
			
			System.out.println();
		}
	}}
/*

01-1
02-2
03-3
04-4
05-5
06-4
07-3
08-2
09-1



*/