
public class MyProg {
	byte a=127;
	short b= 32767;
	int c=2147483647;
	long d=9223372036854775807l;
	String name="Shubham";
	public static void main(String[] args) {
		byte a=127;
		short b= 32767;
		int c=2147483647;
		long d=9223372036854775807l;
		System.out.println(a);
		System.out.println(b);
		System.out.println(c);
		System.out.println(d);
	}

}